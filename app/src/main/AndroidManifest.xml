<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.otec.Tokcoin">

    <uses-permission android:name="android.permission.INTERNET" />


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:screenOrientation="portrait"
        android:supportsRtl="true"
        android:theme="@style/Theme.Crevatech">

        <activity android:name="com.otec.Tokcoin.UI.Register"
                  android:screenOrientation="portrait"/>
        <activity android:name="com.otec.Tokcoin.UI.Report_issues"
                  android:screenOrientation="portrait"/>
        <activity android:name="com.otec.Tokcoin.UI.Shop"
                  android:screenOrientation="portrait"/>
        <activity android:name="com.otec.Tokcoin.UI.Login"
                  android:screenOrientation="portrait"/>
        <activity
            android:name="com.otec.Tokcoin.UI.MainActivity"
            android:exported="true"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>